$('#folder_assets').append("<%= j render partial: 'spree/admin/shared/digital_asset', collection: @digital_assets %>");
<% if @digital_assets.current_page < @digital_assets.total_pages %>
  $('#view_more_button').attr('href', "<%= admin_digital_assets_path folder_id: @current_folder.try(:id), page: (@digital_assets.current_page + 1) %>");
<% else %>
  $('#view_more_button').attr('href', '');
<% end %>
