$('.tree-menu .active').removeClass('active').addClass('inactive');
$(".tree-menu [data-id=<%= @current_folder.try(:id) %>]").parent().addClass('active').removeClass('inactive');

if ($('#folder_assets').length == 0) {
  $('#main-part').html("<%= j render 'content' %>")
}
else {
  $('#folder_assets').html("<%= j render partial: 'spree/admin/shared/digital_asset', collection: @digital_assets %>");
}

$('#asset_folder_id').val("<%= @current_folder.try(:id) %>");
$('#upload_asset_button').removeClass('disabled').removeAttr('disabled');
