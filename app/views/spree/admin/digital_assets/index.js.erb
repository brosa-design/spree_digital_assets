window.scrollTo(0, 0);
$('.tree-menu .active').removeClass('active');
$(".tree-menu [data-id=<%= @current_folder.try(:id) %>]").parent().addClass('active');
$('.sidebar-menu-item .selected').removeClass('selected');
if ($('#folder_assets:visible').length == 0) {
  $('#main-part').html("<%= j render 'content' %>");
} else {
  $('#folder_assets').html("<%= j render partial: 'spree/admin/shared/digital_asset', collection: @digital_assets %>");
}
$('#folder_assets').attr('data-next-url', "<%= digital_assets_next_page_path(@digital_assets, @current_folder) %>");
$('#digital_asset_folder_id').val("<%= @current_folder.try(:id) %>");
$('#upload_asset_button').removeClass('disabled').removeAttr('disabled');
$('.with-tip').tooltip();
