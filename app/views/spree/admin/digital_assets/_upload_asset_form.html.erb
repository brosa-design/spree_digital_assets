<%= form_for [:admin, @digital_asset || Spree::DigitalAsset.new], html: { multipart: true, class: 'form-horizontal', id: 'new_digital_asset_form' }, remote: true, format: :js, authenticity_token: true do |f| %>
  <%= render partial: 'form', locals: { f: f } %>
  <div class="form-group">
    <div class="col-md-offset-4 col-md-6">
      <%= button Spree.t('actions.create'), 'save' %>
    </div>
  </div>
<% end %>